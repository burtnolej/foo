<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui">
	
<ribbon>
		
<tabs>
<tab id="adminTab" label="Admin">
				
<group id="codeTab" label="Code Mgmt">
					
	<button id="DoBackups" label="Backup VBA" onAction="onAction" />

	<button id="GetProcs" label="Get Proc Review" onAction="onAction" />

</group>

</tab>		
<tab id="quadTab" label="Quad">
				
<group id="formsGroup" label="Forms">
					
	<button id="LoadDefinitions" label="Load Definitions" onAction="onAction" />

	<button id="GenerateEntryForm" label="Generate Form" onAction="onAction" />
	
	<button id="DeleteEntryForm" label="Delete Form" onAction="onAction" />
	
</group>

<group id="tablesGroup" label="Tables">
	
	<button id="CreateTables" label="Create Tables" onAction="onAction" />
	<button id="DeleteTables" label="Delete Tables" onAction="onAction" />
	<button id="PersistTables" label="Persist Tables" onAction="onAction" />
	
	<button id="LoadTables" label="Load Tables" onAction="onAction" />
	
	
			
</group>

<group id="newGroup" label="New Records">
					
	<button id="Student" label="Student" onAction="onAction" />

	<button id="Teacher" label="Teacher" onAction="onAction" />
			
</group>

<group id="toggleGroup" label="Toggle">
					
	<button id="CellStyles" label="Cell Styles" onAction="onAction" />

	<button id="Definitions" label="Definitions" onAction="onAction" />
	
	<button id="FormStyles" label="Form Styles" onAction="onAction" />
		
</group>

<group id="viewGroup" label="View Records">
					
	<button id="Students" label="Students" onAction="onAction" />

	<button id="Teachers" label="Teachers" onAction="onAction" />
			
</group>
				
</tab>
		
</tabs>
	
</ribbon>

</customUI>
